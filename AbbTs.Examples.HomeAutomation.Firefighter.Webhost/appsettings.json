{
  "SmartQuartier": {
    "BaseAddress": "http://127.0.0.1:11001/",
    "HistoryPath": "smart-quartier/data-service/history",
    "StatisticPath": "smart-quartier/data-service/statistic",
    "TimeoutSeconds": 10
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SinglePageApps": {
    "/": {
      "DevServerAddress": "http://localhost:3000/",
      "SpaRootPath": "Apps/firefighter-dashboard",
      "NodePackageManager": "Pnpm"
    }
  }
}